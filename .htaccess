RewriteEngine On
RewriteRule ^_showall\/?$ "/Wiki/pages/show_all_page.php" [L]
RewriteRule ^_logout\/?$ "/Wiki/pages/logout_page.php" [L]
RewriteRule ^_login\/?$ "/Wiki/functions/login.php" [L]
RewriteRule ^_settings\/?$ "/Wiki/pages/settings_page.php" [L]
RewriteRule ^_admin\/?$ /Wiki/pages/admin_page.php [L]
RewriteRule ^_create\/?$ "/Wiki/pages/create_page.php" [L]
RewriteRule ^_random\/?$ "/Wiki/pages/random_page.php" [L]
RewriteRule ^_search\/?$ "/Wiki/pages/search_page.php" [L]
RewriteRule ^$ "/Wiki/pages/wiki_page.php?page=home" [L]
RewriteRule ^home\/?$ "/Wiki/pages/wiki_page.php?page=home" [L]
RewriteRule ^([^/]*)/_edit$ /Wiki/pages/edit_page.php?page=$1 [L]
RewriteRule ^([^/]*)/_history$ /Wiki/pages/history_page.php?page=$1 [L]
RewriteRule ^([^/]*)/_show_history$ /Wiki/pages/show_history_page.php [L]
RewriteRule ^([^/]*)/?$ /Wiki/pages/wiki_page.php?page=$1 [L]
